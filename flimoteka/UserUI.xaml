<Window x:Class="flimoteka.UserUI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:flimoteka"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Background="#282828"
        Title="UserUI" Height="720" Width="1280">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="89*"/>
            <ColumnDefinition Width="71*"/>
        </Grid.ColumnDefinitions>
        <DataGrid Foreground="#FF969696" Background="#FF323232" x:Name="FilmList" d:ItemsSource="{d:SampleData ItemCount=5}"/>
        <Image Grid.Column="1" Margin="21,10,21,9" Source="/86.jpg" Stretch="Fill"/>

    </Grid>
</Window>
